  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "noImplicitAny": false,
    "strict": false,
    "module": "es6",
    "downlevelIteration": true,
    "lib": ["es2015", "dom"],
    "target": "es5",
    "allowJs": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
